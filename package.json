{
  "name": "manager",
  "version": "1.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "test": "npm run ng -- test",
    "lint": "npm run ng -- lint",
    "e2e": "npm run ng -- e2e",
    "start": "npm run ng -- serve",
    "build": "npm run ng -- build --stats-json",
    "build:projects": "node ./utils/build-projects.js",
    "build:prod": "npm run ng -- build --prod --source-map --stats-json --build-optimizer false",
    "build:libs:api": "npm run ng -- build @manager/api",
    "build:libs:core": "npm run ng -- build @manager/core",
    "build:libs:components": "npm run ng -- build @manager/components",
    "build:libs:dashboard": "npm run ng -- build @manager/dashboard",
    "build:libs:editor": "npm run ng -- build @manager/editor",
    "watch:libs:api": "npm run ng -- build @manager/api --watch",
    "watch:libs:core": "npm run ng -- build @manager/core --watch",
    "watch:libs:components": "npm run ng -- build @manager/components --watch",
    "watch:libs:dashboard": "npm run ng -- build @manager/dashboard --watch",
    "watch:libs:editor": "npm run ng -- build @manager/editor --watch",
    "build:all": "npm run build:projects && npm run build",
    "bundle-report": "npm run build && webpack-bundle-analyzer dist/project/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~9.0.4",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "firebase": "^7.8.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "3.7.5"
  }
}
