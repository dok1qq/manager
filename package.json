{
	"name": "manager",
	"version": "1.0.0",
	"scripts": {
		"ng": "node ./node_modules/@angular/cli/bin/ng",
		"test": "npm run ng -- test",
		"lint": "npm run ng -- lint",
		"e2e": "npm run ng -- e2e",
		"start": "npm run ng -- serve",
		"build": "npm run ng -- build --stats-json",
		"build:projects": "node ./utils/build-projects.js",
		"build:prod": "npm run ng -- build --prod --source-map --stats-json --build-optimizer false",
		"build:libs:api": "npm run ng -- build @manager/api",
		"build:libs:core": "npm run ng -- build @manager/core",
		"build:libs:components": "npm run ng -- build @manager/components",
		"build:libs:dashboard": "npm run ng -- build @manager/dashboard",
		"watch:libs:api": "npm run ng -- build @manager/api --watch",
		"watch:libs:core": "npm run ng -- build @manager/core --watch",
		"watch:libs:components": "npm run ng -- build @manager/components --watch",
		"watch:libs:dashboard": "npm run ng -- build @manager/dashboard --watch",
		"bundle-report": "npm run build && webpack-bundle-analyzer dist/project/stats-es2015.json"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.14",
		"@angular/cdk": "^8.2.3",
		"@angular/common": "~8.2.14",
		"@angular/compiler": "~8.2.14",
		"@angular/core": "~8.2.14",
		"@angular/forms": "~8.2.14",
		"@angular/material": "^8.2.3",
		"@angular/platform-browser": "~8.2.14",
		"@angular/platform-browser-dynamic": "~8.2.14",
		"@angular/router": "~8.2.14",
		"rxjs": "~6.4.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.803.19",
		"@angular-devkit/build-ng-packagr": "~0.803.22",
		"@angular/cli": "~8.3.19",
		"@angular/compiler-cli": "~8.2.14",
		"@angular/language-service": "~8.2.14",
		"@types/node": "~8.9.4",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"codelyzer": "^5.0.0",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"ng-packagr": "^5.4.0",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tsickle": "^0.37.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3"
	}
}
